{
  "name": "module-users",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/module-users/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/module-users",
        "configFile": "apps/module-users/vite.config.ts"
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "buildTarget": "module-users:build",
        "configFile": "apps/module-users/vite.config.ts"
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "options": {
        "buildTarget": "module-users:build",
        "configFile": "apps/module-users/vite.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "maxWarnings": 0
      }
    },
    "lint:fix": {
      "executor": "@nx/eslint:lint",
      "options": {
        "fix": true
      }
    },
    "test": {
      "executor": "@nx/vitest:test",
      "options": {
        "configFile": "apps/module-users/vitest.config.ts",
        "passWithNoTests": true
      }
    },
    "test:coverage": {
      "executor": "@nx/vitest:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "configFile": "apps/module-users/vitest.config.ts",
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/apps/module-users",
        "coverage": true
      }
    },
    "test:ci": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest run --coverage --reporter=dot --passWithNoTests",
        "cwd": "apps/module-users"
      }
    },
    "test:watch": {
      "executor": "@nx/vitest:test",
      "options": {
        "configFile": "apps/module-users/vitest.config.ts",
        "passWithNoTests": true,
        "watch": true
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vue-tsc --noEmit -p apps/module-users/tsconfig.app.json"
      }
    }
  }
}
